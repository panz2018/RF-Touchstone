{
  "name": "rf-touchstone",
  "type": "module",
  "version": "0.0.6",
  "description": "A Javascript/TypeScript library for reading, manipulating, and writing Touchstone files (.snp files) used in radio frequency (RF) and microwave engineering.",
  "main": "dist/Touchstone.cjs.js",
  "module": "dist/Touchstone.es.js",
  "browser": "dist/Touchstone.umd.js",
  "unpkg": "dist/Touchstone.umd.js",
  "jsdelivr": "dist/Touchstone.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/Touchstone.umd.js",
      "import": "./dist/Touchstone.es.js",
      "require": "./dist/Touchstone.cjs.js"
    }
  },
  "files": [
    "./dist",
    "./src",
    "./readme.md",
    "./LICENSE.md",
    "./coverage/coverage-badge.svg",
    "./.github/AI_USAGE_GUIDE.md"
  ],
  "license": "MIT",
  "author": "https://github.com/panz2018",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panz2018/RF-Touchstone.git"
  },
  "keywords": [
    "rf",
    "touchstone",
    "snp",
    "s1p",
    "s2p",
    "s3p",
    "s4p",
    "s5p",
    "s6p",
    "frequency",
    "scattering",
    "matrix",
    "impedance"
  ],
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "test": "run-p --print-label lint format test:unit test:coverage build && run-s --print-label docs:md docs:build",
    "lint": "eslint . --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage && coverage-badges --source coverage/coverage-summary.json --output coverage/coverage-badge.svg",
    "build": "tsc && vite build && yarn pack",
    "docs:md": "typedoc --tsconfig tsconfig.json",
    "docs:dev": "yarn --cwd examples/react build && node docs/.vitepress/copy-docs.mjs && vitepress dev docs",
    "docs:build": "yarn --cwd examples/react build && node docs/.vitepress/copy-docs.mjs && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "mathjs": "15.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.2.1",
    "@vitest/coverage-v8": "4.0.18",
    "coverage-badges-cli": "2.2.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "happy-dom": "20.5.0",
    "markdown-it-mathjax3": "5.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.8.1",
    "rollup-plugin-visualizer": "6.0.5",
    "typedoc": "0.28.16",
    "typedoc-plugin-markdown": "4.10.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitepress": "1.6.4",
    "vitest": "4.0.18"
  }
}
